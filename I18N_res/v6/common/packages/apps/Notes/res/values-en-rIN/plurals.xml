<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android"
           xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <plurals name="note_count_in_folder">
        <item quantity="one">1 note</item>
        <item quantity="other"><xliff:g id="NUMBER">%1$s</xliff:g> notes</item>
    </plurals>
    <plurals name="search_results_title">
        <item quantity="one"><xliff:g id="number" example="1">%1$s</xliff:g> result for \"<xliff:g id="search" example="???">%2$s</xliff:g>\"</item>
        <!-- Case of 0 or 2 or more results. -->
        <item quantity="other"><xliff:g id="number" example="15">%1$s</xliff:g> results for \"<xliff:g id="search" example="???">%2$s</xliff:g>\"</item>
    </plurals>
    <plurals name="note_terms">
        <item quantity="one"><xliff:g id="count">%1$s</xliff:g> note</item>
        <item quantity="other"><xliff:g id="count">%1$s</xliff:g> notes</item>
    </plurals>
    <plurals name="folder_terms">
        <item quantity="one"><xliff:g id="count">%1$s</xliff:g> folder</item>
        <item quantity="other"><xliff:g id="count">%1$s</xliff:g> folders</item>
    </plurals>
</resources>