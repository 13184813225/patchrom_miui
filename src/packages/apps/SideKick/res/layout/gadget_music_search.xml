<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">
    <TextView
        style="@style/MusicSearchResultText"
        android:id="@+id/result_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/music_result_title"/>
    <TextView
        style="@style/MusicCategoryText"
        android:id="@+id/local_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/local_category"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/local_result_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/local_result_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/local_result_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/MusicCategoryText"
        android:id="@+id/online_category"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/online_category"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/online_result_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/online_result_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        style="@style/MusicItemText"
        android:id="@+id/online_result_3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</LinearLayout>