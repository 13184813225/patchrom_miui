<?xml version="1.0" encoding="utf-8"?>

<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:settings="http://schemas.android.com/apk/res/com.android.settings"
        android:title="@string/preferred_network_type_title_miui">
    <com.android.phone.MiuiSimPreference
        android:key="pref_sim_info"
        android:layout="@layout/siminfo_select"
        android:persistent="false" />
    <miui.preference.RadioButtonPreferenceCategory
        android:key="network_type_list_global">
        <miui.preference.RadioButtonPreference
            android:key="7"
            android:persistent="false"
            android:title="@string/preferred_network_type_pref_entries_global_miui" />
    </miui.preference.RadioButtonPreferenceCategory>
    <miui.preference.RadioButtonPreferenceCategory
        android:title="@string/preferred_network_type_cdma_miui"
        android:key="network_type_list_cdma">
        <miui.preference.RadioButtonPreference
            android:key="4"
            android:persistent="false"
            android:title="@string/preferred_network_type_pref_entries_auto_miui" />
    </miui.preference.RadioButtonPreferenceCategory>
</PreferenceScreen>

