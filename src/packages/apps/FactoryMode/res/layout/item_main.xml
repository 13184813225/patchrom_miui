<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical" >    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/keys_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/keys_test" />
	        <Button
	            android:id="@+id/backlight_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Backlight_test" />
	        <Button
	            android:id="@+id/calibration_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/calibration_test" />
	        <Button
	            android:id="@+id/T_card_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/T_card_test" />
	    </LinearLayout>    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/Bluetooth_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Bluetooth_test" />
	        <Button
	            android:id="@+id/SimCard_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/SimCard_test" />
	        <Button
	            android:id="@+id/Vibrator_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Vibrator_test"/>
	        <Button
	            android:id="@+id/RTC_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/RTC_test" />
	    </LinearLayout>    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/Ringtone_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Ringtone_test"/>
	        <Button
	            android:id="@+id/Receiver_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Receiver_test" />
	        <Button
	            android:id="@+id/Headset_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Headset_test" />
	        <Button
	            android:id="@+id/Led_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Led_test" />
	    </LinearLayout>    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/Fm_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Fm_test" />
	        <Button
	            android:id="@+id/Camera_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Camera_test" />
	        <Button
	            android:id="@+id/Battery_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Battery_test" />
	        <Button
	            android:id="@+id/Wifi_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Wifi_test" />
	    </LinearLayout>    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/Torch_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Torch_test" />
			<Button
	            android:id="@+id/Loopback_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Loopback_test" />

	        <Button
	            android:id="@+id/Lcd_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
 	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/LCD_test" />	         
	    </LinearLayout> 
	    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
            <Button
	            android:id="@+id/gps_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/gps_button" />
	            
	        <Button
	            android:id="@+id/G_sensor_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/G_sensor_test" />
	       
	        <Button
	            android:id="@+id/Gyro_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/Gyro_test" />
	    </LinearLayout>
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/proximity_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/proximity_sensor_button" />
	        <Button
	            android:id="@+id/optic_sensor_button"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/optic_sensor_button" />
	    </LinearLayout>
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:gravity="center"
	        android:orientation="horizontal" >
	        <Button
	            android:id="@+id/magnetic_btn"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/magnetic_test" />
	        <Button
	            android:id="@+id/Gsensor_Calibrate_btn"
	            android:layout_width="0dp"
	            android:layout_weight="1"
	            android:layout_height="wrap_content"
	            android:gravity="center"
	            android:text="@string/gsensor_calibrate_title" />
	    </LinearLayout>
	    
    </LinearLayout>
              
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:orientation="horizontal" >
        <Button
            android:id="@+id/back_button"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/back_return" />
    </LinearLayout>                
</LinearLayout>
