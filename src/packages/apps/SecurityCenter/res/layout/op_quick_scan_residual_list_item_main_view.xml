<?xml version="1.0" encoding="utf-8"?>
<com.miui.optimizecenter.view.QuickScanResidualListItemMainView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ListItemMainView" >

    <ImageView
        android:id="@+id/arrow"
        style="@style/ListItemMainViewArrow" />

    <ImageView
        android:id="@+id/icon"
        style="@style/ListItemMainViewIcon"
        android:layout_toRightOf="@id/arrow" />

    <CheckBox
        android:id="@+id/checkbox"
        style="@style/ListItemMainViewCheckbox" />

    <RelativeLayout
        style="@style/ListItemMainViewContent"
        android:layout_toLeftOf="@id/checkbox"
        android:layout_toRightOf="@id/icon" >

        <TextView
            android:id="@+id/title"
            style="@style/ListItemMainViewTextPrimary" />

        <TextView
            android:id="@+id/summary"
            style="@style/ListItemMainViewTextSecondary"
            android:layout_below="@id/title" />
    </RelativeLayout>

</com.miui.optimizecenter.view.QuickScanResidualListItemMainView>