<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- wifi to wlan -->
    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_accessWifiState">view WLAN state</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_accessWifiState">Allows an application to view
      the information about the state of WLAN.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_changeWifiState">change WLAN state</string>
    <!-- Description of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permdesc_changeWifiState">Allows an application to connect
      to and disconnect from WLAN access points, and to make changes to
      configured WLAN networks.</string>

    <!-- Title of an application permission, listed so the user can choose whether they want to allow the application to do this. -->
    <string name="permlab_changeWifiMulticastState">allow WLAN Multicast
      reception</string>
  <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's title. -->
    <plurals name="wifi_available">
        <item quantity="one">WLAN network available</item>
        <item quantity="other">WLAN networks available</item>
    </plurals>
    <!-- A notification is shown when there are open wireless networks nearby.  This is the notification's message. -->
    <plurals name="wifi_available_detailed">
        <item quantity="one">Open WLAN network available</item>
        <item quantity="other">Open WLAN networks available</item>
    </plurals>

    <!-- A notification is shown when a captive portal network is detected.  This is the notification's title. -->
    <string name="wifi_available_sign_in">Sign in to WLAN network</string>
    <!-- A notification is shown when a user's selected SSID is later disabled due to connectivity problems.  This is the notification's title / ticker. -->
    <string name="wifi_watchdog_network_disabled">Couldn\'t connect to WLAN</string>
     <!-- Wi-Fi p2p dialog title-->
    <string name="wifi_p2p_dialog_title">WLAN Direct</string>
    <string name="wifi_p2p_turnon_message">Start WLAN Direct operation. This will turn off WLAN client/hotspot operation.</string>
    <string name="wifi_p2p_failed_message">Couldn\'t start WLAN Direct</string>
    <string name="wifi_p2p_enabled_notification_title">WLAN Direct is on</string>
     <!-- Notification title when Wi-Fi data usage has exceeded limit threshold, and has been disabled. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_title">WLAN data disabled</string>
    <!-- Notification title when Wi-Fi data usage has exceeded limit threshold. [CHAR LIMIT=32] -->
    <string name="data_usage_wifi_limit_snoozed_title">WLAN data limit exceeded</string>
</resources>
