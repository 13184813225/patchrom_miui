<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 以下是MIUI新加的颜色定义，原生系统没有单独提取出这些颜色定义 -->
    <add-resource type="color" name="notification_action_text_color" />
    <add-resource type="color" name="statusbar_content" />
    <add-resource type="color" name="statusbar_content_title" />
    <add-resource type="color" name="statusbar_content_info" />
    <add-resource type="color" name="statusbar_content_time" />
    <add-resource type="color" name="statusbar_content_emphasis" />
    <add-resource type="color" name="toast_text_color" />

    <!-- 输入框选中内容的提示颜色 -->
    <color name="notification_action_text_color">#ccc</color>
    <color name="statusbar_content">#80ffffff</color>
    <color name="statusbar_content_title">#ffffff</color>
    <color name="statusbar_content_info">#80ffffff</color>
    <color name="statusbar_content_time">#80ffffff</color>
    <color name="statusbar_content_emphasis">#CCCCCC</color>

    <drawable name="notification_bg">#00000000</drawable>
    <drawable name="notification_bg_low">#00000000</drawable>

    <color name="toast_text_color">#ccffffff</color>

    <color name="highlighted_text_light">#4df06410</color>
    <color name="highlighted_text_dark">#4df06410</color>
</resources>
